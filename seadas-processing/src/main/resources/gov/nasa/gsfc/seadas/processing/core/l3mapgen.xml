        <option type="string">
            <name>projection</name>
            <value>platecarree</value>
            <default></default>
            <description>proj.4 projection string or one of the following predefined projections:</description>
            <source>default</source>
            <validValues>
                <validValue>
                    <value></value>
                    <description>proj.4 projection string or a predefined l3mapgen projection shortcut</description>
                </validValue>
                <validValue>
                    <value>smi</value>
                    <description><![CDATA[Standard Mapped image, cylindrical projection, uses central_meridian, projection="+proj=eqc +lon_0=<central_meridian>"]]></description>
                </validValue>
                <validValue>
                    <value>platecarree</value>
                    <description><![CDATA[ Plate Carree image, cylindrical projection, uses central_meridian, projection="+proj=eqc +lon_0=<central_meridian>"]]></description>
                </validValue>
                <validValue>
                    <value>mollweide</value>
                    <description><![CDATA[Mollweide projection projection="+proj=moll +lon_0=<central_meridian>"]]></description>
                </validValue>
                <validValue>
                    <value>lambert</value>
                    <description><![CDATA[Lambert conformal conic projection, uses central_meridian, projection="+proj=lcc +lon_0=<central_meridian>"]]></description>
                </validValue>
                <validValue>
                    <value>albersconic</value>
                    <description><![CDATA[Albers equal-area conic projection, uses central_meridian projection="+proj=aea +lon_0=<central_meridian>"]]></description>
                </validValue>
                <validValue>
                    <value>mercator</value>
                    <description><![CDATA[ Mercator cylindrical map projection, uses central_meridian projection="+proj=merc +lon_0=<central_meridian>"]]></description>
                </validValue>
                <validValue>
                    <value>ease2</value>
                    <description><![CDATA[Ease Grid 2 projection, uses central_meridian,  projection="+proj=cea +lon_0=0 +lat_ts=30 +ellps=WGS84 +datum=WGS84 +units=m +lat_0=<central_meridian>"]]></description>
                </validValue>
                <validValue>
                    <value>conus</value>
                    <description>USA Contiguous Albers Equal Area Conic USGS version</description>
                </validValue>
                <validValue>
                    <value>raw</value>
                    <description>raw dump of the bin file</description>
                </validValue>



                <validValue>
                    <value>#### Proj4 Projection Expressions ####</value>
                    <description>Albers Equal Area</description>
                </validValue>


                <validValue>
                    <value>+proj=aea +lon_0=0.0 +lat_1=0.0 +lat_2=0.0</value>
                    <description>Albers Equal Area</description>
                </validValue>

                <validValue>
                    <value>+proj=aeqd +lon_0=0.0 +lat=0.0</value>
                    <description>Azimuthal Equidistant</description>
                </validValue>

                <validValue>
                    <value>+proj=aitoff +lon_0=0.0</value>
                    <description>Aitoff</description>
                </validValue>

                <validValue>
                    <value>+proj=cea +lon_0=0.0 +lat_ts=30.0</value>
                    <description>Equal Area Cylindrical (Behrmann at 30 degrees)</description>
                </validValue>

                <validValue>
                    <value>+proj=cea +lon_0=0.0</value>
                    <description>Lambert Cylindrical Equal Area</description>
                </validValue>

                <validValue>
                    <value>+proj=crast +lon_0=0.0</value>
                    <description>Craster Parabolic (Putnins P4)</description>
                </validValue>

                <validValue>
                    <value>+proj=eck4 +lon_0=0.0</value>
                    <description>Eckert IV Projection</description>
                </validValue>

                <validValue>
                    <value>+proj=eqc +lon_0=0.0</value>
                    <description>Equidistant Cylindrical (Plate Carrée)</description>
                </validValue>

                <validValue>
                    <value>+proj=eqdc +lon_0=0.0 +lat_1=0.0 +lat_2=0.0</value>
                    <description>&lt;html&gt;
                        Equidistant Conic &lt;br&gt;
                        &lt;/html&gt;</description>
                </validValue>

                <validValue>
                    <value>+proj=eqearth +lon_0=0.0</value>
                    <description>Equal Earth Projection</description>
                </validValue>

                <validValue>
                    <value>+proj=geos +lon_0=0.0 +h=705000.0</value>
                    <description>&lt;html&gt;
                        Geostationary Satellite View &lt;br&gt;
                        Example shown at orbit altitude of 705km
                        &lt;/html&gt;</description>
                </validValue>

                <validValue>
                    <value>+proj=hammer +lon_0=0.0</value>
                    <description>Hammer &amp; Eckert-Greifendorff</description>
                </validValue>

                <validValue>
                    <value>+proj=igh +lon_0=0.0</value>
                    <description>Interrupted Goode Homolosine</description>
                </validValue>

                <validValue>
                    <value>+proj=igh_o +lon_0=0.0</value>
                    <description>Interrupted Goode Homolosine (Oceanic View)</description>
                </validValue>

                <validValue>
                    <value>+proj=laea +lon_0=0.0 +lat_0=0.0</value>
                    <description>Lambert Azimuthal Equal Area</description>
                </validValue>

                <validValue>
                    <value>+proj=lcc +lon_0=0.0 +lat_0=0.0 +lat_1=0.0 +lat_2=0.0</value>
                    <description>&lt;html&gt;
                        Lambert Conformal Conic &lt;br&gt;
                        &lt;/html&gt;</description>
                </validValue>

                <validValue>
                    <value>+proj=leac +lon_0=0.0 +lat_1=0.0</value>
                    <description>&lt;html&gt;
                        Lambert Equal Area Conic &lt;br&gt;
                        &lt;/html&gt;</description>
                </validValue>

                <validValue>
                    <value>+proj=leac +lon_0=0.0 +lat_1=0.0 +south</value>
                    <description>&lt;html&gt;
                        Lambert Equal Area Conic (South) &lt;br&gt;
                        &lt;/html&gt;</description>
                </validValue>

                <validValue>
                    <value>+proj=merc +lon_0=0.0 +lat_ts=0.0</value>
                    <description>&lt;html&gt;
                        Mercator &lt;br&gt;
                        &lt;/html&gt;</description>
                </validValue>

                <validValue>
                    <value>+proj=moll +lon_0=0.0</value>
                    <description>Mollweide</description>
                </validValue>

                <validValue>
                    <value>+proj=natearth +lon_0=0.0</value>
                    <description>Natural Earth</description>
                </validValue>

                <validValue>
                    <value>+proj=natearth2 +lon_0=0.0</value>
                    <description>Natural Earth II</description>
                </validValue>

                <validValue>
                    <value>+proj=nsper +lon_0=0.0 +lat_0=0.0</value>
                    <description>Near-sided perspective</description>
                </validValue>

                <validValue>
                    <value>+proj=ortho +lon_0=0.0 +lat_0=0.0</value>
                    <description>Orthographic</description>
                </validValue>

                <validValue>
                    <value>+proj=patterson +lon_0=0.0</value>
                    <description>Patterson</description>
                </validValue>

                <validValue>
                    <value>+proj=putp2 +lon_0=0.0</value>
                    <description>Putnins P2</description>
                </validValue>

                <validValue>
                    <value>+proj=qua_aut +lon_0=0.0</value>
                    <description>Quartic Authalic</description>
                </validValue>

                <validValue>
                    <value>+proj=robin +lon_0=0.0</value>
                    <description>Robinson Projection</description>
                </validValue>

                <validValue>
                    <value>+proj=sinu +lon_0=0.0</value>
                    <description>Sinusoidal</description>
                </validValue>

                <validValue>
                    <value>+proj=stere +lon_0=0.0 +lat_0=0.0</value>
                    <description>&lt;html&gt;
                        Stereographic  &lt;br&gt;
                        &lt;/html&gt;</description>
                </validValue>

                <validValue>
                    <value>+proj=sterea +lon_0=0.0 +lat_0=0.0</value>
                    <description>&lt;html&gt;
                        Oblique Stereographic Alternative &lt;br&gt;
                        &lt;/html&gt;</description>
                </validValue>

                <validValue>
                    <value>+proj=tcea +lon_0=0.0</value>
                    <description>&lt;html&gt;
                        Transverse Cylindrical Equal Area &lt;br&gt;
                        &lt;/html&gt;</description>
                </validValue>

                <validValue>
                    <value>+proj=tmerc +lon_0=0.0 +lat_0=0.0</value>
                    <description>&lt;html&gt;
                        Transverse Mercator &lt;br&gt;
                        &lt;/html&gt;</description>
                </validValue>

                <validValue>
                    <value>+proj=ups +lon_0=0.0</value>
                    <description>&lt;html&gt;
                        Universal Polar Stereographic &lt;br&gt;
                        &lt;/html&gt;</description>
                </validValue>

                <validValue>
                    <value>+proj=wag4 +lon_0=0.0</value>
                    <description>Wagner IV</description>
                </validValue>

                <validValue>
                    <value>+proj=wintri +lon_0=0.0</value>
                    <description>&lt;html&gt;
                        Winkel Tripel &lt;br&gt;
                        &lt;/html&gt;</description>
                </validValue>

            </validValues>
            <usedAs>option</usedAs>
            <colSpan>3</colSpan>
            <order>3</order>
        </option>


