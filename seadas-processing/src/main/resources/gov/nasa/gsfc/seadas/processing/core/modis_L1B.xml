<?xml version="1.0" encoding="ISO-8859-1"?>
<paramInfo xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:noNamespaceSchemaLocation="https://seadas.gsfc.nasa.gov/software/schemas/ParamInfo-1.0.xsd">>
    <programMetaData>
        <hasParFile>false</hasParFile>
        <parFileOptionName>parfile</parFileOptionName>
        <hasGeoFile>true</hasGeoFile>
        <primaryOptions>
            <primaryOption>ifile</primaryOption>
            <primaryOption>geofile</primaryOption>
        </primaryOptions>
        <progressRegex>scan: (\d+) out of (\d+)</progressRegex>
    </programMetaData>
    <options>
        <option type="ifile">
            <name>ifile</name>
            <value></value>
            <default></default>
            <description>Input L1A file</description>
            <source>command line</source>
            <usedAs>argument</usedAs>
            <order>0</order>
        </option>
        <option type="ifile">
            <name>geofile</name>
            <value></value>
            <default></default>
            <description>geo file</description>
            <source>command line</source>
            <usedAs>argument</usedAs>
            <order>1</order>
        </option>
        <option type="ofile">
            <name>--okm</name>
            <value></value>
            <default></default>
            <description>Output MODIS L1B 1KM HDF filename</description>
            <usedAs>option</usedAs>
            <order>2</order>
        </option>
        <option type="ofile">
            <name>--hkm</name>
            <value></value>
            <default></default>
            <description>Output MODIS L1B HKM HDF filename</description>
            <usedAs>option</usedAs>
            <order>3</order>
        </option>
        <option type="ofile">
            <name>--qkm</name>
            <value></value>
            <default></default>
            <description>Output MODIS L1B QKM HDF filename</description>
            <usedAs>option</usedAs>
            <order>4</order>
        </option>
        <option type="ofile">
            <name>--obc</name>
            <value></value>
            <default></default>
            <description>Output MODIS L1B OBC HDF filename</description>
            <usedAs>option</usedAs>
            <order>5</order>
        </option>
        <option type="string">
            <name>--lutver</name>
            <value></value>
            <default></default>
            <description>L1B LUT version number</description>
            <usedAs>option</usedAs>
            <order>6</order>
        </option>
        <option type="dir">
            <name>--lutdir</name>
            <value></value>
            <default></default>
            <description>Path of directory containing LUT files</description>
            <usedAs>option</usedAs>
            <order>7</order>
        </option>
        <option type="boolean">
            <name>--del-okm</name>
            <value></value>
            <default></default>
            <description>Delete 1km resolution L1B file</description>
            <usedAs>flag</usedAs>
            <order>8</order>
        </option>
        <option type="boolean">
            <name>--del-hkm</name>
            <value></value>
            <default></default>
            <description>Delete 500m resolution L1B file</description>
            <usedAs>flag</usedAs>
            <order>9</order>
        </option>
        <option type="boolean">
            <name>--del-qkm</name>
            <value></value>
            <default></default>
            <description>Delete 250m resolution L1B file</description>
            <usedAs>flag</usedAs>
            <order>10</order>
        </option>
        <option type="boolean">
            <name>--keep-obc</name>
            <value></value>
            <default></default>
            <description>Save onboard calibration file</description>
            <usedAs>flag</usedAs>
            <order>11</order>
        </option>
        <option type="boolean">
            <name>--log</name>
            <value></value>
            <default></default>
            <description>Save processing log file(s)</description>
            <usedAs>flag</usedAs>
            <order>12</order>
        </option>
        <option type="boolean">
            <name>--verbose</name>
            <value>true</value>
            <default></default>
            <description>Path of directory containing LUT files</description>
            <usedAs>flag</usedAs>
            <order>13</order>
        </option>
        <option type="button">
            <name>update luts</name>
            <value></value>
            <default></default>
            <description>update luts for a given mission. MISSION is either seawifs, aqua, or terra
            </description>
            <usedAs>button</usedAs>
            <order>14</order>
        </option>
    </options>
