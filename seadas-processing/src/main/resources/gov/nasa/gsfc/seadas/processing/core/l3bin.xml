                <value></value>
                <description>bin resolution</description>
            </validValue>
            <validValue>
                <value>HH</value>
                <description>50m</description>
            </validValue>
            <validValue>
                <value>HQ</value>
                <description>100m</description>
            </validValue>
            <validValue>
                <value>Q</value>
                <description>250m</description>
            </validValue>
            <validValue>
                <value>H</value>
                <description>500m</description>
            </validValue>
            <validValue>
                <value>1</value>
                <description>1.1km</description>
            </validValue>
            <validValue>
                <value>2</value>
                <description>2.3km</description>
            </validValue>
            <validValue>
                <value>4</value>
                <description>4.6km</description>
            </validValue>
            <validValue>
                <value>9</value>
                <description>9.2km</description>
            </validValue>
            <validValue>
                <value>18</value>
                <description>18.5km</description>
            </validValue>
            <validValue>
                <value>36</value>
                <description>36km</description>
            </validValue>
            <validValue>
                <value>QD</value>
                <description>0.25 degree</description>
            </validValue>
            <validValue>
                <value>HD</value>
                <description>0.5 degree</description>
            </validValue>
            <validValue>
                <value>1D</value>
                <description>1 degree</description>
            </validValue>
            <usedAs>option</usedAs>
            <colSpan>2</colSpan>
            <order>20</order>
        </option>


